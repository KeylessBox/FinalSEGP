<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<HBox prefHeight="38.0" prefWidth="230.0" styleClass="custom_hbox" stylesheets="@../css/case.css"
      xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Pane prefHeight="38.0" prefWidth="2.0" styleClass="indicator" stylesheets="@../css/case.css">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </Pane>
        <VBox prefHeight="40.0" prefWidth="170.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="custom" alignment="TOP_CENTER" prefHeight="14.0" prefWidth="32.0" text="new">
                            <font>
                                <Font name="Calibri Bold" size="11.0"/>
                            </font>
                            <padding>
                                <Insets bottom="1.0"/>
                            </padding>
                            <HBox.margin>
                                <Insets left="2.0" top="3.0"/>
                            </HBox.margin>
                        </Label>
                        <Label fx:id="caseName" alignment="BOTTOM_LEFT" prefHeight="17.0" prefWidth="113.0"
                               text="Cheeky Scar Suka">
                            <font>
                                <Font name="Calibri Bold" size="15.0"/>
                            </font>
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Label>
                    </children>
                </HBox>
                <Label prefHeight="13.0" prefWidth="149.0" text="Last time updated at: 15:15:2017"
                       textAlignment="CENTER" textFill="#909090">
                    <padding>
                        <Insets left="1.0"/>
                    </padding>
                    <font>
                        <Font name="Calibri Bold Italic" size="10.0"/>
                    </font>
                    <VBox.margin>
                        <Insets bottom="1.0" left="4.0"/>
                    </VBox.margin>
                </Label>
            </children>
            <padding>
                <Insets left="10.0" top="4.0"/>
            </padding>
        </VBox>
        <HBox prefHeight="26.0" prefWidth="48.0">
            <children>
                <Button minHeight="12.0" minWidth="15.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0"
                        styleClass="button_edit" stylesheets="@../css/case.css">
                    <HBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="8.0"/>
                    </HBox.margin>
                    <tooltip>
                        <Tooltip text="Edit the case"/>
                    </tooltip>
                </Button>
                <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0" styleClass="button_delete"
                        stylesheets="@../css/case.css">
                    <HBox.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="8.0"/>
                    </HBox.margin>
                    <tooltip>
                        <Tooltip text="Delete the case"/>
                    </tooltip>
                </Button>
            </children>
            <padding>
                <Insets top="2.0"/>
            </padding>
        </HBox>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</HBox>
