                                <ToggleButton id="custom" mnemonicParsing="false" prefHeight="4.0" prefWidth="40.0" text="New">
                                    <font>
                                        <Font name="Calibri Bold" size="12.0" />
                                    </font>
                                    <padding>
                                        <Insets bottom="2.0" />
                                    </padding>
                                    <HBox.margin>
                                        <Insets left="4.0" top="2.0" />
                                    </HBox.margin>
                                    <toggleGroup>
                                        <ToggleGroup fx:id="custom" />
                                    </toggleGroup>
                                    <tooltip>
                                        <Tooltip text="Change case status to New" />
                                    </tooltip>
                                </ToggleButton>
                                <ToggleButton id="custom2" mnemonicParsing="false" prefHeight="4.0" prefWidth="40.0" text="Done">
