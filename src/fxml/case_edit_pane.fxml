                                <Insets top="2.0" />
                            </padding>
                        </TextField>
                    </children>
                    <padding>
                        <Insets left="5.0" top="2.0" />
                    </padding>
                </VBox>
                <Button mnemonicParsing="false" prefHeight="22.0" prefWidth="23.0" styleClass="button_ok" stylesheets="@../css/case_edit.css">
