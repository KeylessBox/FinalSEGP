                                    <HBox.margin>
                                        <Insets top="2.0" />
                                    </HBox.margin>
                                    <tooltip>
                                        <Tooltip text="Change case status to Done" />
                                    </tooltip>
                                </ToggleButton>
                            </children>
                        </HBox>
                        <TextField prefHeight="19.0" prefWidth="139.0">
